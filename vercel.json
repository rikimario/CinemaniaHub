{
  "version": 2,
  "builds": [
    {
      "src": "src/main.jsx",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.jsx",
      "methods": ["GET", "PUT", "PATCH", "POST", "DELETE", "OPTIONS"],
      "headers": { "Access-Control-Allow-Origin": "*" }
    }
  ]
}
